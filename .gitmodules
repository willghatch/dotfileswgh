### emacs

### These top few are the most important to have my core functionality.
[submodule "external/emacs/repeatable-motion"]
	path = external/emacs/repeatable-motion
	url = https://github.com/willghatch/emacs-repeatable-motion
[submodule "external/emacs/hydra"]
	path = external/emacs/hydra
	url = https://github.com/abo-abo/hydra
## the queue package is required by undo-tree
[submodule "external/emacs/queue"]
	path = external/emacs/queue
	url = https://github.com/emacs-straight/queue
[submodule "external/emacs/undo-tree"]
	path = external/emacs/undo-tree
	url = https://gitlab.com/tsc25/undo-tree

